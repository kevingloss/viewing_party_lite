<h1>Viewing Party Lite</h1>

<% if current_user  %>
  <%= link_to "Logout", '/logout', method: :delete %>
<% else %>
  <%= link_to 'Create New User', new_user_path %>
  <%= link_to 'Log In', login_path %>
<% end %>

<h3>Existing Users</h3>
<% @users.each do |user| %>
    <p><%= link_to "#{user.name} | #{user.email}", dashboard_path %></p>
<% end %>
